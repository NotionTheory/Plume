{
  "name": "plume",
  "private": true,
  "version": "0.0.7",
  "title": "Plume VR",
  "description": "Instant VR meetings in your browser.",
  "themeColor": "purple",
  "homepage": "https://www.plumevr.com",
  "twitter": "@NotionTheory",
  "keywords": [
    "WebGL",
    "WebVR",
    "HTML5",
    "JavaScript",
    "virtual reality",
    "Primrose",
    "teleconferencing",
    "WebRTC",
    "calls"
  ],
  "GoogleWebVROriginTrialToken": "<check back later>",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "node server.js --mode=dev --path=.. --url=Plume/",
    "legend": "node server.js --mode=dev --path=.. --url=Legend3D/",
    "photo": "node server.js --mode=dev --path=.. --url=photoVR/",
    "jwd": "node server.js --mode=dev --path=.. --url=JWD/",
    "primrose": "node server.js --mode=dev --path=.. --url=Primrose/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NotionTheory/Plume.git"
  },
