  "dependencies": {
    "azure-storage": "^0.10.0",
    "notion-node": "0.0.1",
    "opentok": "^2.4.0",
    "primrose": "^0.28.0"
  },
  "devDependencies": {
    "notiontheory-basic-build": "^2.6.0"
  }
